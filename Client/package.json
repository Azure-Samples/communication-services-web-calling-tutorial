{
  "name": "client",
  "version": "0.3.0",
  "private": true,
  "dependencies": {
    "@azure/communication-calling": "^1.7.0-beta.1",
    "@azure/communication-common": "^2.1.0",
    "@fluentui/react": "^8.86.3",
    "@uifabric/fluent-theme": "^7.7.7",
    "@uifabric/icons": "^7.7.4",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "source-map-explorer": "^2.5.2",
    "uuid": "^8.3.2"
  },
  "scripts": {
    "build": "npm run clean && concurrently --kill-others-on-fail \"webpack --mode=production --env production\"",
    "clean": "rimraf dist",
    "package": "copyfiles -E -u 3 \"../Server/dist/**/*\" dist",
    "start": "concurrently \"cd ../Server && npm run start:dev\" \"npm run start:app\"",
    "start:app": "webpack-dev-server --port 5000 --mode development",
    "test": "",
    "prettier": "prettier --no-error-on-unmatched-pattern --write --config ./.prettierrc --ignore-path=../.prettierignore \"**/*.js\" \"**/*.jsx\"",
    "prettier:check": "prettier --no-error-on-unmatched-pattern --check --config ./.prettierrc --ignore-path=./.prettierignore \"**/*.js\" \"**/*.jsx\"",
    "lint": "eslint */**/*.{js,jsx}",
    "lint:fix": "eslint */**/*.{js,jsx} --fix",
    "lint:quiet": "eslint */**/*.{js,jsx} --quiet",
    "analyze": "source-map-explorer dist/build/build.js"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.18.9",
    "@webpack-cli/serve": "^1.7.0",
    "babel-loader": "^8.0.6",
    "concurrently": "^7.3.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "html-loader": "^4.1.0",
    "html-webpack-plugin": "^5.5.0",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "webpack": "^5.74.0",
    "webpack-dev-server": "^4.9.3"
  },
  "peerDependencies": {
    "@azure/logger": "^1.0.2"
  }
}
